{
    "manifest_version": 2,
  
    "name": "Discarded Tab App",
    "description": "This extension displays the discarded chrome tabs",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "tabs"
    ]
}